{
    "name": "coopcycle/vroom-php",
    "type": "library",
    "description": "PHP library for VROOM",
    "license": "MIT",
    "authors" : [{
        "name": "Andrew Cunningham",
        "email": "andrewjcunnin@gmail.com"
    }, {
        "name": "Alexandre Segura",
        "email": "mex.zktk@gmail.com"
    }],
    "support": {
        "issues" : "https://github.com/coopcycle/vroom-php/issues",
        "source" : "https://github.com/coopcycle/vroom-php"
    },
    "require": {
        "php": "^7.0",
        "willdurand/geocoder": "^4.2",
        "symfony/serializer": "^4.0 || ^5.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "^6.5.5 || ^7.0"
    },
    "autoload" : {
        "psr-4" : {
            "Vroom\\": "src",
            "Vroom\\Tests\\": "tests"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "0.1-dev"
        }
    }
}
